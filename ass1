CREATE TABLE Employee(
ID INT,
Name VARCHAR(50),
Email VARCHAR(100),
Department VARCHAR(50),
Date_of_Birth DATE,
Salary DECIMAL(10,2),
Is_Active BOOLEAN
) ;
 
